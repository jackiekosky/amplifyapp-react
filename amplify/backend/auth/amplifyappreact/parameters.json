{
  "authSelections": "userPoolOnly",
  "resourceName": "amplifyappreact",
  "serviceType": "imported",
  "region": "us-east-2",
  "aliasAttributes": [
    "preferred_username"
  ],
  "requiredAttributes": [
    "given_name",
    "family_name",
    "email",
    "phone_number"
  ],
  "passwordPolicyMinLength": 8,
  "passwordPolicyCharacters": [
    "Requires Lowercase",
    "Requires Uppercase",
    "Requires Numbers",
    "Requires Symbols"
  ],
  "mfaConfiguration": "OPTIONAL",
  "autoVerifiedAttributes": [
    "email"
  ],
  "mfaTypes": [
    "TOTP"
  ]
}